body    { font-size: 12px; margin-top: 1px; margin-left:0px; }
td      { font-size: 12px; bgcolor: #D2D2D2}
input   { font-size: 12px; color: #000000; vertical-align: middle }
select  { font-size: 12px; color: #000000; vertical-align: middle }

a       { text-decoration: none; color: #000000; cursor: pointer }
a:hover { text-decoration: underline; color: #0000ff; cursor: pointer }

A.A1          { COLOR: #000000; TEXT-DECORATION: none }
A.A1:visited  { COLOR: #990033; TEXT-DECORATION: underline }
A.A1:hover    { COLOR: #0000ff; TEXT-DECORATION: underline }

A.AUL         { COLOR: #000000; TEXT-DECORATION: underline }
A.AUL:visited { COLOR: #666666; TEXT-DECORATION: underline }
A.AUL:hover   { COLOR: #ff3366; TEXT-DECORATION: none }

A.AUL01         { COLOR: #0000cc; TEXT-DECORATION: underline }
A.AUL01:hover   { COLOR: #3399ff; TEXT-DECORATION: none }

A.AUL02         { COLOR: #ffffff; TEXT-DECORATION: underline }
A.AUL02:hover   { COLOR: #3399ff; TEXT-DECORATION: none }

A.AUL03         { COLOR: #009900; TEXT-DECORATION: underline }
A.AUL03:hover   { COLOR: #3399ff; TEXT-DECORATION: none }

A.title_link         { COLOR: #FF6600; TEXT-DECORATION: underline ; font-weight: bold }
A.title_link:visited { COLOR: #FF6600; TEXT-DECORATION: underline; font-weight: bold }
A.title_link:hover   { COLOR: #3333FF; TEXT-DECORATION: none; font-weight: bold }

A.textlink         { font-size: 12px; font-weight: bold; color: #0000cc; text-decoration: underline }
A.textlink:visited { font-size: 12px; font-weight: bold; color: #0000cc; text-decoration: underline }
A.textlink:hover   { font-size: 12px; font-weight: bold; color: #3399ff; text-decoration: none }

A.email         { font-size: 12px; font-family: Verdana; font-weight: bold; color: #0000cc; text-decoration: underline }
A.email:visited { font-size: 12px; font-family: Verdana; font-weight: bold; color: #0000cc; text-decoration: underline }
A.email:hover   { font-size: 12px; font-family: Verdana; font-weight: bold; color: #3399ff; text-decoration: none }


A.gray_link         { font-size: 12px; color: #666666; text-decoration: underline }
A.gray_link:visited { font-size: 12px; color: #666666; text-decoration: underline }
A.gray_link:hover   { font-size: 12px; color: #666666; text-decoration: underline }

A.bottom_link         { font-size: 12px; color: #FFFFFF; text-decoration: none }
A.bottom_link:visited { font-size: 12px; color: #FFFFFF; text-decoration: none }
A.bottom_link:hover   { font-size: 12px; color: #706600; text-decoration: none }

A.white_link         { color: #FFFFFF; text-decoration: underline }
A.white_link:visited { color: #FFFFFF; text-decoration: underline }
A.white_link:hover   { color: #FFFFFF; text-decoration: underline }

A.green_link         { color: #706600; text-decoration: underline }
A.green_link:visited { color: #706600; text-decoration: underline }
A.green_link:hover   { color: #706600; text-decoration: underline }

A.deepblue_link         { color: #003366; text-decoration: underline }
A.deepblue_link:visited { color: #003366; text-decoration: underline }
A.deepblue_link:hover   { color: #003366; text-decoration: underline }

A.cust_link         { color: #CC4D4D; text-decoration: underline }
A.cust_link:visited { color: #CC4D4D; text-decoration: underline }
A.cust_link:hover   { color: #CC4D4D; text-decoration: underline }

