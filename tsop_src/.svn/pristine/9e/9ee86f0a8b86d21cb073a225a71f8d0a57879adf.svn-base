.pacrudMenuBox {
	box-shadow: 4px 4px 4px #BBBBBB;
}

.pacrudMenu {
	display:block;
	cursor:pointer;
	background-color: #BEE09D;
	text-indent:5px;
}

.pacrudMenuL1 {
	cursor:pointer;
	text-indent:5px;
	
	background: #dce8d0; /* Old browsers */
	background: -webkit-linear-gradient(left,  #dce8d0 0%,#ebffe5 100%); /* Chrome10+,Safari5.1+ */
	background: -ms-linear-gradient(left,  #dce8d0 0%,#ebffe5 100%); /* IE10+ */
	background: linear-gradient(to right,  #dce8d0 0%,#ebffe5 100%); /* W3C */
}
.pacrudMenuL1:hover {
	background:rgba(252,255,255,0)
}

